<?xml version='1.0' encoding='utf-8'?>
<widget id="kopin88.v2m.acc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>v2m-acc</name>
    <description>
      V2M COLTD
    </description>
    <author email="hlaingmyohtun.hmh@gmail.com" href="https://www.facebook.com/hlainggyi88">
      Vue.js + Onsen UI + Cordova teams
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-camera" spec="^4.0.2" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.1" />
    <preference name="StatusBarBackgroundColor" value="#187bd1" />
    <engine name="android" spec="~7.1.2" />
</widget>
